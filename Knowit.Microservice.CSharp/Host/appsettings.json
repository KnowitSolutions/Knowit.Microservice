{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "NodeUris": "http://localhost:9200",
          "IndexFormat": "projectname-{0:yyyy.MM.dd}",
          "EmitEventFailure": "ThrowException"
        }
      }
    ]
  }
}