{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://#{Hosting.BindAddress | JsonEscape}"
      }
    }
  },
  "ConnectionStrings" : {
    "Database": "#{ConnectionStrings.Database | JsonEscape}"
  },
  "Database": {
    "Migrate": "#{Database.Migrate | JsonEscape}",
    "Path": "#{Database.Path | JsonEscape}"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "#{Log.Path | JsonEscape}/ProjectName.log",
          "rollingInterval": "#{Log.RolloverInterval | JsonEscape}"
        }
      }
    ]
  }
}